<body>
    <div class="page-wrapper">
        <main class="main">
            <div class="page-content">
                <div class="container">
                	<div class="row">
                		<div class="col-lg-10">        
                            <div class="products mb-3">
                                 {{#each result}}
                                <div class="product product-list">
                                     
                                    <div class="row">
                                        <div class="col-6 col-lg-3">
                                                <span class="product-label label-new">{{this.productname}}</span>
                                                <a href="/product">
                                                    <img src="/images/product/{{this.img.[3]}}"  style="width:200px ; height: 200px;" alt="Product image" class="product-image">
                                                </a>
                                            
                                        </div><!-- End .col-sm-6 col-lg-3 -->

                                        <div class="col-6 col-lg-3 order-lg-last">
                                            <div class="product-list-action">
                                                <div class="product-price">
                                                    ${{this.price}}
                                                </div><!-- End .product-price -->
                                                <div class="ratings-container">
                                                    <div class="ratings">
                                                        <div class="ratings-val" style="width: 20%;"></div><!-- End .ratings-val -->
                                                    </div><!-- End .ratings -->
                                                    <span class="ratings-text">( 2 Reviews )</span>
                                                </div><!-- End .rating-container -->

                                                  <button class="btn btn-outline-dark" onclick="moveToWishlist('{{this._id}}')"><span>MOVE TO WISHLIST</span></button>
                                            </div><!-- End .product-list-action -->
                                        </div><!-- End .col-sm-6 col-lg-3 -->

                                        <div class="col-lg-6">
                                            <div class="product-body product-action-inner">
                                                
                                                <h3 class="product-title"><a href="/product-view/{{this._id}}">{{this.productname}}</a></h3><!-- End .product-title -->

                                                <div class="product-content">
                                                    <p>{{this.Description}}</p>
                                                </div><!-- End .product-content -->
                                                
                                                <div class="product-nav product-nav-thumbs">
                                                    <a href="#" class="active">
                                                        <img src="/images/product/{{this.img.[1]}}"  style="width:40px ; height: 40px;" alt="product desc">
                                                    </a>
                                                    <a href="#">
                                                        <img src="/images/product/{{this.img.[2]}}"  style="width:40px ; height: 40px;" alt="product desc">
                                                    </a>

                                                    <a href="#">
                                                        <img src="/images/product/{{this.img.[0]}}"  style="width:40px ; height: 40px;" alt="product desc">
                                                    </a>
                                                </div><!-- End .product-nav -->
                                            </div><!-- End .product-body -->
                                        </div><!-- End .col-lg-6 -->
                                    </div><!-- End .row -->
                                       
                                </div><!-- End .product -->
                               {{/each}}
                            </div>  
                		</div><!-- End .col-lg-9 -->
                		<aside class="col-lg-2 order-lg-first">
                			<div class="sidebar sidebar-shop">
									<div class="collapse show" id="widget-1">
										<div class="widget-body">
											<div class="filter-items filter-items-count">

											</div><!-- End .filter-items -->
										</div><!-- End .widget-body -->
									</div><!-- End .collapse -->
        						</div><!-- End .widget -->
                               
        						<div class="widget widget-collapsible">

        						</div><!-- End .widget -->
                </div><!-- End .container -->
            </div><!-- End .page-content -->
        </main><!-- End .main -->
